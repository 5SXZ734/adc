			OPND_F0
			OPND1(OPND_1)
IT_END(FADD)

// F A D D (2)
IT_FRM2(FADD,2)//"Add Real"
	{OP_ST,		OP_STi},	//f[0]=f[0]+f[i]
	{OP_STi,	OP_ST},		//f[i]=f[i]+f[0]
IT_BEG2(FADD,2)
//op1 = op1+op2
	ACTN1(ACTN_MOV)
		OPND1(OPND_1)
		ACTN1(ACTN_ADD)
			OPND1(OPND_1)
			OPND1(OPND_2)
IT_END2(FADD,2)

// F A D D P (1)
IT_FRM(FADDP)//"Add Real & Pop"
	{OP_STi,	OP_ST},		//f[-i]=f[-i]+f[0]; f=f+1
IT_BEG(FADDP)
//op1 = op1+op2, FTOP++
	ACTN4(ACTN_MOV, 0, _FTOP(1))
		OPND1(OPND_1)
		ACTN1(ACTN_ADD)
			OPND1(OPND_1)
			OPND1(OPND_2)
IT_END(FADDP)

