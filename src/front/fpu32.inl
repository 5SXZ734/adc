IT_END(FABS)

// F A D D (1)
IT_FRM(FADD)//"Add Real"
	{OP_M32REAL},
	{OP_M64REAL},
	{OP_STi},
IT_BEG(FADD)
//f[0] = f[0]+op1
	ACTN1(ACTN_MOV)
		OPND_F0
